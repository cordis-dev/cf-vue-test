{
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 2015
    },
    "parser": "vue-eslint-parser",
    "plugins": [
        "import",
        "vue"
    ],
	"extends":[
		"plugin:vue/recommended",
		"@vue/eslint-config-standard"
	],
    "settings": {
        "import/extensions": [
            ".js",
            ".vue"
        ]
    }
}
